{
    "name": "flextype-plugins/sitemap",
    "type": "project",
    "description": "Sitemap plugin provide automatically generated XML sitemap for Flextype.",
    "keywords": ["sitemap", "plugin", "flextype"],
    "homepage": "https://github.com/flextype-plugins/sitemap",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Romanenko",
            "email": "awilum@msn.com",
            "homepage": "https://awilum.github.io/flextype"
        }
    ],
    "support": {
      "issues": "https://github.com/flextype-plugins/sitemap/issues"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*"
    },
    "config": {
        "apcu-autoloader": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "classmap": [
            "src"
        ]
    }
}
